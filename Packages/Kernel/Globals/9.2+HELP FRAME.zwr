^DIC(9.2,92,2,0)="^9.22^6^3"
^DIC(9.2,92,2,4,0)="USING THE SPOOLER^96^"
^DIC(9.2,92,2,5,0)="INSTALLATION^93^"
^DIC(9.2,92,2,6,0)="SPOOL MANAGEMENT^94^"
