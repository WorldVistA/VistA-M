^DIC("394.71","%D",0)="^^52^52^2930927^"
^DIC("394.71","%D",1,0)="This file defines each data segment currently supported by PDX."
^DIC("394.71","%D",2,0)=" "
^DIC("394.71","%D",3,0)=" "
^DIC("394.71","%D",4,0)="Global format of Extraction Array:"
^DIC("394.71","%D",5,0)="     Root@(""VALUE"",file,field,sequence number)=value"
^DIC("394.71","%D",6,0)="     Root@(""ID"",file,field,sequence number)=identifier"
^DIC("394.71","%D",7,0)=" "
^DIC("394.71","%D",8,0)="Extraction methods must account for data encryption."
^DIC("394.71","%D",9,0)=" "
^DIC("394.71","%D",10,0)="Display methods must take the output of their associated extraction method"
^DIC("394.71","%D",11,0)="as input."
^DIC("394.71","%D",12,0)=" "
^DIC("394.71","%D",13,0)="Global format of Display Array:"
^DIC("394.71","%D",14,0)="     Root@(""DISPLAY"",line number,0)=information displayed on specified"
^DIC("394.71","%D",15,0)="                                    line number of screen"
^DIC("394.71","%D",16,0)=" "
^DIC("394.71","%D",17,0)="Methods must use the following parameters (where needed):"
^DIC("394.71","%D",18,0)="     ROOT - Where to store the information (full global reference)"
^DIC("394.71","%D",19,0)="     DFN - Internal file number for requested patient (pointer to PATIENT"
^DIC("394.71","%D",20,0)="           File)"
^DIC("394.71","%D",21,0)="     SEGPTR - Internal file number of segment (pointer to VAQ - DATA"
^DIC("394.71","%D",22,0)="              SEGMENT File)"
^DIC("394.71","%D",23,0)="     XTRCT - Where the extracted information resides (full global"
^DIC("394.71","%D",24,0)="             reference)"
^DIC("394.71","%D",25,0)="     OFFSET - Where line numbering should begin from"
^DIC("394.71","%D",26,0)="     TIMLIM - Time limit to apply to extraction"
^DIC("394.71","%D",27,0)="     OCCLIM - Occurrence limit to apply to extraction"
^DIC("394.71","%D",28,0)=" "
^DIC("394.71","%D",29,0)="Methods must return the following:"
^DIC("394.71","%D",30,0)="     0 - Extraction was successful (if an Extraction Array was returned)"
^DIC("394.71","%D",31,0)="     n - Number of lines in display (if a Display Array was returned)"
^DIC("394.71","%D",32,0)="     -1^ErrorText - Method was not successful"
^DIC("394.71","%D",33,0)=" "
^DIC("394.71","%D",34,0)="Sample methods:"
^DIC("394.71","%D",35,0)="     $$EXTRACT^VAQZZZ(DFN,SEGPTR,TIMLIM,OCCLIM,ROOT,OFFSET) - Display"
^DIC("394.71","%D",36,0)="                                                              Array is"
^DIC("394.71","%D",37,0)="                                                              returned"
^DIC("394.71","%D",38,0)="     $$EXTRACT^VAQZZZ(DFN,SEGPTR,TIMLIM,OCCLIM,ROOT)  - Extraction Array"
^DIC("394.71","%D",39,0)="                                                        is returned"
^DIC("394.71","%D",40,0)="     $$DISPLAY^VAQZZZ(XTRCT,SEGPTR,ROOT,OFFSET) - Display Array, built from"
^DIC("394.71","%D",41,0)="                                                  input extraction array,"
^DIC("394.71","%D",42,0)="                                                  is returned"
^DIC("394.71","%D",43,0)=" "
^DIC("394.71","%D",44,0)="Sample use:"
^DIC("394.71","%D",45,0)="     S ROOT=""^TMP(""""VAQ"""",""_$J_"")"""
^DIC("394.71","%D",46,0)="     S SEGPTR=internal file number for segment"
^DIC("394.71","%D",47,0)="     S DFN=internal file number of patient"
^DIC("394.71","%D",48,0)="     S TIMLIM=time limit to use when extracting segment (if applicable)"
^DIC("394.71","%D",49,0)="     S OCCLIM=occurrence limit to use when extracting segment (if"
^DIC("394.71","%D",50,0)="              applicable)"
^DIC("394.71","%D",51,0)="     X (""S X=""_^VAT(394.71,SEGPTR,""XRTN""))"
^DIC("394.71","%D",52,0)="     The variable X is now set to the proper exit code (X<0 on error)"
^DIC("394.72",0)="VAQ - ENCRYPTION METHOD^394.72"
