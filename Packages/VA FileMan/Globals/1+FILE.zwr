^DIC("1.5217",0,"LAYGO")="^"
^DIC("1.5217",0,"WR")="^"
^DIC("1.5217","%D",0)="^^6^6^2970806^^"
^DIC("1.5217","%D",1,0)="A set of formatting and physical structure specifications."
^DIC("1.5217","%D",2,0)="Each column specification has a column type table element"
^DIC("1.5217","%D",3,0)="(SQLI_TABLE_ELEMENT) which contains the relational specifications, name"
^DIC("1.5217","%D",4,0)="and domain. The column specification contains those attributes required"
^DIC("1.5217","%D",5,0)="to locate the value in the global structure, and to project the value to"
^DIC("1.5217","%D",6,0)="the user."
^DIC("1.5218",0)="SQLI_PRIMARY_KEY^1.5218"
