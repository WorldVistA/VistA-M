^DIC("78.1",0)="COMPLICATION TYPES^78.1"
^DIC("78.1",0,"GL")="^RA(78.1,"
^DIC("78.1","%",0)="^1.005^1^1"
^DIC("78.1","%",1,0)="RA"
^DIC("78.1","%","B","RA",1)=""
