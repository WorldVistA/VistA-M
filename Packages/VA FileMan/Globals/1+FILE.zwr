^DIC("394.62","%D",1,0)="This file holds any patient information that was transmitted using PDX."
^DIC("394.62","%D",2,0)=" "
^DIC("394.62","%D",3,0)=" "
^DIC("394.62","%D",4,0)="If field # .05 is set to 'YES', fields # .03, .04, 10, 20, and 30 will not"
^DIC("394.62","%D",5,0)="contain data (field # 50 will contain the display ready format of the"
^DIC("394.62","%D",6,0)="segment)."
^DIC("394.62","%D",7,0)=" "
^DIC("394.62","%D",8,0)="If field # .05 is set to 'NO', field # 50 will not contain data (fields #"
^DIC("394.62","%D",9,0)=".03, .04, 10, 20, and 30 will contain the information required to rebuild"
^DIC("394.62","%D",10,0)="the Extraction Array)."
^DIC("394.62","%D",11,0)=" "
^DIC("394.62","%D",12,0)="Segments stored as display ready will only have one entry."
^DIC("394.71",0)="VAQ - DATA SEGMENT^394.71I"
^DIC("394.71",0,"AUDIT")="@"
^DIC("394.71",0,"DD")="@"
^DIC("394.71",0,"DEL")="@"
