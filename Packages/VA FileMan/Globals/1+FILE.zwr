^DIC(798,0)="ROR REGISTRY RECORD^798"
^DIC(798,0,"AUDIT")=""
^DIC(798,0,"DD")="@"
^DIC(798,0,"DEL")="@"
^DIC(798,0,"GL")="^RORDATA(798,"
