^DIC(774,"%D",1,0)="This file is used to control the flow of messages from one system to"
^DIC(774,"%D",2,0)="another. It is currently used by CIRN to permit sites to subscribe via HL7"
^DIC(774,"%D",3,0)="Master File updates to patient information. This subscription 'request'"
^DIC(774,"%D",4,0)="message contains the data necessary to update the Subscription Control"
^DIC(774,"%D",5,0)="file via the api:"
^DIC(774,"%D",6,0)=" "
^DIC(774,"%D",7,0)="UPD^HLSUB(HLSCN,HLNN,HLTP,HLAD,HLTD,HLRAP,.HLER)"
^DIC(774,"%D",8,0)=" "
^DIC(774,"%D",9,0)="HLSCN-subscription control number"
^DIC(774,"%D",10,0)="HLNN-Logical Link name"
^DIC(774,"%D",11,0)="HLTP-subscription type"
^DIC(774,"%D",12,0)="       0-patient descriptive only"
^DIC(774,"%D",13,0)="       1-patient clinical and descriptive"
^DIC(774,"%D",14,0)="       2-other (locally defined)"
^DIC(774,"%D",15,0)="HLAD-activation date"
^DIC(774,"%D",16,0)="HLTD-termination date (optional)"
^DIC(774,"%D",17,0)="HLRAP-HL7 receiving application"
^DIC(774,"%D",18,0)="HLER-error messages (pass by reference)"
^DIC(774,"%D",19,0)=" "
^DIC(774,"%D",20,0)="The patient file maintains a pointer to this file. When a clinical event"
^DIC(774,"%D",21,0)="takes place pertaining to a particular patient, the subscription control"
^DIC(774,"%D",22,0)="number is looked up in the patient file, then a call is made to return the"
^DIC(774,"%D",23,0)="current list of subscribers using:"
^DIC(774,"%D",24,0)=" "
^DIC(774,"%D",25,0)="GET^HLSUB(HLSCN,HLTP,HLCL,.HLL)"
^DIC(774,"%D",26,0)=" "
^DIC(774,"%D",27,0)="HLSCN-subscription control number"
^DIC(774,"%D",28,0)="HLTP-subscription type (null returns ALL)"
