^DIC("3.8","%",0)="^1.005^1^1"
^DIC("3.8","%",1,0)="XM"
^DIC("3.8","%","B","XM",1)=""
^DIC("3.8","%D",0)="^^2^2^2881108^^^^"
^DIC("3.8","%D",1,0)="This file holds the names of all groups known to MailMan, and their"
^DIC("3.8","%D",2,0)="members."
^DIC("3.816",0)="DISTRIBUTION LIST^3.816"
^DIC("3.816",0,"GL")="^XMB(3.816,"
^DIC("3.816","%D",0)="^^12^12^2931204^^^"
^DIC("3.816","%D",0,"LE")="1"
^DIC("3.816","%D",1,0)="The Distribution List file has entries that consist of names.  Each name is"
^DIC("3.816","%D",2,0)="associated with one or more domain.  When a Distribution List is entered"
^DIC("3.816","%D",3,0)="into a Mail Group, MailMan will deliver the message to all the entities"
^DIC("3.816","%D",4,0)="it has linked to it.  A Distribution List is interpreted as a name at which"
^DIC("3.816","%D",5,0)="the message will be delivered to at each of the associated domains in the"
^DIC("3.816","%D",6,0)="list.  Therefore a Distribution List whose NAME is G.SUPPORT and whose"
^DIC("3.816","%D",7,0)="associated domains are DOMAIN.EXT, ALTOONA,DOMAIN,EXT and ISC-SF.DOMAIN.EXT will"
^DIC("3.816","%D",8,0)="be sent (in addition to all other entities attached to the Mail Group) to:"
^DIC("3.816","%D",9,0)=" "
^DIC("3.816","%D",10,0)="    G.SUPPORT@DOMAIN.EXT"
^DIC("3.816","%D",11,0)="    G.SUPPORT@ALTOONA.DOMAIN.EXT"
^DIC("3.816","%D",12,0)=" &  G.SUPPORT@ISC-SF.DOMAIN.EXT"
^DIC("3.9",0)="MESSAGE^3.9s"
^DIC("3.9",0,"GL")="^XMB(3.9,"
^DIC("3.9","%",0)="^1.005^1^1"
^DIC("3.9","%",1,0)="XM"
^DIC("3.9","%","B","XM",1)=""
^DIC("3.9","%D",0)="^^1^1^2940407^^^^"
^DIC("3.9","%D",0,"LE")="1"
^DIC("3.9","%D",1,0)="These messages are the heart of the MailMan system."
^DIC("4.281",0)="INTER-UCI TRANSFER^4.281"
^DIC("4.281",0,"GL")="^%ZISL(4.281,"
^DIC("4.281","%",0)="^1.005^1^1"
^DIC("4.281","%",1,0)="XM"
^DIC("4.281","%","B","XM",1)=""
^DIC("4.281","%D",0)="^^5^5^3020422^^"
^DIC("4.281","%D",1,0)="The Inter-UCI Transfer file is used by MailMan to do non-interactive"
^DIC("4.281","%D",2,0)="transmissions and receptions of messages to and from UCIs that share"
^DIC("4.281","%D",3,0)="the %ZISL file either because they are on the same machine or because"
^DIC("4.281","%D",4,0)="they are on the same local area network and use DDP translation techniques"
^DIC("4.281","%D",5,0)="for the %ZISL global."
^DIC("4.2995",0)="MAILMAN OUTSTANDING FTP TRANSACTIONS^4.2995"
^DIC("4.2995",0,"GL")="^XMBX(4.2995,"
^DIC("4.2995","%D",0)="^^5^5^2931204^"
^DIC("4.2995","%D",1,0)="When multimedia messages are transmitted, procedure files are created to"
^DIC("4.2995","%D",2,0)="send the 'non-textual message body parts' using the File Transfer Protocol"
^DIC("4.2995","%D",3,0)="(FTP).  Entries are made into this file for each such instance and then"
^DIC("4.2995","%D",4,0)="the TCP/IP Poller will execute the procedure file and delete the entry for"
^DIC("4.2995","%D",5,0)="it."
^DIC("4.2997",0)="REMOTE USER DIRECTORY^4.2997I"
^DIC("4.2997",0,"GL")="^XMD(4.2997,"
^DIC("4.2997","%",0)="^1.005^1^1"
^DIC("4.2997","%",1,0)="XM"
^DIC("4.2997","%","B","XM",1)=""
^DIC("4.2997","%D",0)="^^3^3^3020422^^^"
^DIC("4.2997","%D",1,0)="People who have mailboxes on remote systems may be entered into this"
^DIC("4.2997","%D",2,0)="file so that local users can look up their names and have their mail"
^DIC("4.2997","%D",3,0)="automatically addressed."
^DIC("4.2998",0)="MESSAGE DELIVERY STATS^4.2998D"
^DIC("4.2998",0,"GL")="^XMBX(4.2998,"
^DIC("4.2998","%",0)="^1.005^1^1"
^DIC("4.2998","%",1,0)="XM"
^DIC("4.2998","%","B","XM",1)=""
^DIC("4.2998","%D",0)="^^2^2^3020422^^^^"
^DIC("4.2998","%D",1,0)="This file is used to store statistics about local message deliveries,"
^DIC("4.2998","%D",2,0)="response time and numbers of active user processes."
