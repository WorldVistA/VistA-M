^DIC("9.9",0)="PATCH MONITOR^9.9"
^DIC("9.9",0,"AUDIT")="#"
^DIC("9.9",0,"DD")="@ "
^DIC("9.9",0,"DEL")="@ "
^DIC("9.9",0,"GL")="^XPD(9.9,"
^DIC("9.9",0,"LAYGO")="@ "
