^DIC("356.11","%D",7,0)=" "
^DIC("356.11","%D",8,0)="Per VHA Directive 10-93-142, this file definition should not be modified."
^DIC("356.19",0)="CLAIMS TRACKING UNBILLED AMOUNTS DATA^356.19"
^DIC("356.19",0,"AUDIT")="@"
^DIC("356.19",0,"DD")="@"
^DIC("356.19",0,"DEL")="@"
^DIC("356.19",0,"GL")="^IBE(356.19,"
