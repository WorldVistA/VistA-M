^DIC(9000001,0)="PATIENT/IHS^9000001sIP"
^DIC(9000001,0,"DD")="@"
^DIC(9000001,0,"DEL")="@"
^DIC(9000001,0,"GL")="^AUPNPAT("
^DIC(9000001,"%D",0)="^^10^10^2960809^^^^"
^DIC(9000001,"%D",1,0)="This file is IHS's primary patient data file.  The NAME (.01) field of this "
^DIC(9000001,"%D",2,0)="file is a pointer to the VA's patient file (#2).  Fields in "
^DIC(9000001,"%D",3,0)="common between the two dictionaries actually exist only in the VA patient "
^DIC(9000001,"%D",4,0)="file and are referenced by the IHS patient file as computed fields.  All "
^DIC(9000001,"%D",5,0)="other files containing patient data have backward pointers linking them to "
^DIC(9000001,"%D",6,0)="this file.  The linkage is by patient name and the internal FileMan gener-"
^DIC(9000001,"%D",7,0)="ated number of the ancillary file is the same number used in this file."
^DIC(9000001,"%D",8,0)="   "
^DIC(9000001,"%D",9,0)="All applications developed for the RPMS which require patient data will"
^DIC(9000001,"%D",10,0)="point to this file."
^DIC(9000010,0)="VISIT^9000010sID"
^DIC(9000010,0,"DD")="@"
^DIC(9000010,0,"GL")="^AUPNVSIT("
^DIC(9000010,"%D",0)="^^9^9^2960809^^^^"
^DIC(9000010,"%D",1,0)="This file contains a record of all patient visits at health care"
^DIC(9000010,"%D",2,0)="facilities or by health care providers, including direct outpatient"
^DIC(9000010,"%D",3,0)="and clinic visits, as well as inpatient encounters with providers of care."
^DIC(9000010,"%D",4,0)="All other "
^DIC(9000010,"%D",5,0)="visit related files, such as purpose of visit (diagnoses), operative"
^DIC(9000010,"%D",6,0)="procedures, immunizations, examinations, etc. will point to a visit"
^DIC(9000010,"%D",7,0)="in this file.  The records are maintained by date/time of visit, and"
^DIC(9000010,"%D",8,0)="the patient name field is a pointer to the IHS Patient file,"
^DIC(9000010,"%D",9,0)="where the patient must exist before data can be added here."
^DIC(9000010,"%D",10,0)="Cross References:"
^DIC(9000010,"%D",11,0)="   (""AA"",<patient pointer>,<9's-visit date+time>,IEN)=<eligibility pointer>"
^DIC(9000010,"%D",12,0)="   (""AAH"",<patient pointer>,<9's-visit date+time>,IEN)="""""
^DIC(9000010,"%D",13,0)="      set conditional SERVICE CATEGORY = Hospitalization"
^DIC(9000010,"%D",14,0)="   (""ABILL"",<date visit created>, IEN)="""""
^DIC(9000010,"%D",15,0)="      set conditional to site parameter"
^DIC(9000010,"%D",16,0)="   (""AC"",<patient pointer>,IEN)="""""
^DIC(9000010,"%D",17,0)="   (""AD"", <parent visit pointer>,IEN)="""""
^DIC(9000010,"%D",18,0)="   (""ADEL"",<visit date&time>,IEN)="""""
^DIC(9000010,"%D",19,0)="      set conditional DEPENDENT ENTRY COUNT '> 0"
^DIC(9000010,"%D",20,0)="   (""AI"",IEN)="""""
^DIC(9000010,"%D",21,0)="      set conditional SERVICE CATEGORY = In hospital"
^DIC(9000010,"%D",22,0)="   (""AMRG"",<date visit created>,IEN)="""""
^DIC(9000010,"%D",23,0)="   (""APCIS"",<date visit created>,IEN)="""""
^DIC(9000010,"%D",24,0)="  "
^DIC("9000010.06",0)="V PROVIDER^9000010.06IP"
^DIC("9000010.06",0,"DD")="@"
^DIC("9000010.06",0,"GL")="^AUPNVPRV("
^DIC("9000010.06","%D",0)="^^11^11^2960809^^"
^DIC("9000010.06","%D",1,0)="This file has been designed for joint use by the Indian Health Service and"
^DIC("9000010.06","%D",2,0)="the Department of Veteran Affairs."
^DIC("9000010.06","%D",3,0)=" "
^DIC("9000010.06","%D",4,0)="In IHS, this record, along with a purpose of visit, is required for each"
^DIC("9000010.06","%D",5,0)="patient encounter at a facility, or at a visit in the home or other field"
^DIC("9000010.06","%D",6,0)="location.  This same requirement does not exist in the VA."
^DIC("9000010.06","%D",7,0)=" "
^DIC("9000010.06","%D",8,0)="Data must exist in both the Patient/IHS file and the Visit file before"
^DIC("9000010.06","%D",9,0)="data can be entered here.  There can be multiple providers for a given"
^DIC("9000010.06","%D",10,0)="visit. The primary/secondary field identifies which provider is considered"
^DIC("9000010.06","%D",11,0)="the primary provider for this visit."
^DIC("9000010.07",0)="V POV^9000010.07IP"
^DIC("9000010.07",0,"DD")="@"
^DIC("9000010.07",0,"GL")="^AUPNVPOV("
^DIC("9000010.07","%D",0)="^^47^47^2960809^^^^"
^DIC("9000010.07","%D",1,0)="This file has been designed for joint use by the Indian Health Service and"
^DIC("9000010.07","%D",2,0)="the Department of Veteran Affairs.  POV is an abbreviation for ""Purpose of"
^DIC("9000010.07","%D",3,0)="Visit"" (descriptive name used by IHS) or ""Problem of Visit"" (descriptive"
^DIC("9000010.07","%D",4,0)="name used by VA)."
^DIC("9000010.07","%D",5,0)=" "
^DIC("9000010.07","%D",6,0)="The V POV file is used to store clinical data related to the ""purpose of"
^DIC("9000010.07","%D",7,0)="visit"" or ""problem of visit"", (POV). This is the provider's definition of"
^DIC("9000010.07","%D",8,0)="what diagnosis to use to represent the patient care given at the visit."
^DIC("9000010.07","%D",9,0)="The POV entry is not the patient's ""Chief Complaint"" text.  It is the"
^DIC("9000010.07","%D",10,0)="diagnosis as defined by the provider which will have an ICD Diagnosis"
^DIC("9000010.07","%D",11,0)="code related to it to support Clinical needs and additionally support"
^DIC("9000010.07","%D",12,0)="Administrative functions too such as Billing, Workload, and DSS.  "
^DIC("9000010.07","%D",13,0)=" "
^DIC("9000010.07","%D",14,0)="There should be at least one ""purpose of visit"" (descriptive name used by"
^DIC("9000010.07","%D",15,0)="IHS) or one ""problem of visit"" (descriptive name used by the VA) in the V"
^DIC("9000010.07","%D",16,0)="POV file for each patient visit whether it is an inpatient, outpatient or"
^DIC("9000010.07","%D",17,0)="field visit, and regardless of the discipline of the provider (i.e."
^DIC("9000010.07","%D",18,0)="dental, CHN, mental health, etc.). There is no limit to the number of"
^DIC("9000010.07","%D",19,0)="POV's that can be entered for a patient for a given encounter."
^DIC("9000010.07","%D",20,0)="  "
^DIC("9000010.07","%D",21,0)="At IHS facilities, POV's are generated automatically for this file at"
^DIC("9000010.07","%D",22,0)="time of discharge from the Admission, Discharge and Transfer (ADT) system."
^DIC("9000010.07","%D",23,0)="POV's are entered in narrative form, and coded automatically to the "
^DIC("9000010.07","%D",24,0)="appropriate ICD diagnosis code. Physician entered narrative which modifies"
^DIC("9000010.07","%D",25,0)="diagnosis, such as ""doubtful, suspect, resolved"" are entered by the data"
^DIC("9000010.07","%D",26,0)="entry person in the MODIFIER field.  Narrative qualifiers, such as ""not"
^DIC("9000010.07","%D",27,0)="healing well"", ""date of onset"", ""severe"" etc.. are stored in the NARRATIVE"
^DIC("9000010.07","%D",28,0)="QUALIFIER field.  STAGE is used only as a local option.  The file contains"
^DIC("9000010.07","%D",29,0)="pointers to the IHS Patient file, and visit file, and data must exist in"
^DIC("9000010.07","%D",30,0)="both of these files for this visit before a POV can be entered here."
^DIC("9000010.07","%D",31,0)=" "
^DIC("9000010.07","%D",32,0)="At VA facilities, POV is used as an abbreviation for ""Problem of Visit"","
^DIC("9000010.07","%D",33,0)="or the problem treated at the visit.  POV's are primarily created"
^DIC("9000010.07","%D",34,0)="for clinic visits from 3 sources:"
^DIC("9000010.07","%D",35,0)="  1) The scheduling checkout process, in which case the information"
^DIC("9000010.07","%D",36,0)="collected about the POV is limited to the ICD Diagnosis code.  The"
^DIC("9000010.07","%D",37,0)="provider narrative becomes the ICD narrative from the ICD Diagnosis file."
^DIC("9000010.07","%D",38,0)="  2) The Encounter Form automated data scanning (AICS package).  In this"
^DIC("9000010.07","%D",39,0)="case the provider narrative is the terminology defined by the clinician to"
^DIC("9000010.07","%D",40,0)="represent the diagnosis on the encounter form. The AICS package, or other"
^DIC("9000010.07","%D",41,0)="automated data capture tool, is able to pass the narrative and the ICD"
^DIC("9000010.07","%D",42,0)="Diagnosis.  If the problem treated at the visit was a pre-existing problem"
^DIC("9000010.07","%D",43,0)="from the patient's ""Problem List"", the related problem entry is also"
^DIC("9000010.07","%D",44,0)="stored in the POV record. (The Problem List orientation is not utilized by"
^DIC("9000010.07","%D",45,0)="IHS.)"
^DIC("9000010.07","%D",46,0)="  3) The manual data entry process for encounter form data not collected"
^DIC("9000010.07","%D",47,0)="via automated data capture.  This process is the most like the process IHS"
^DIC("9000010.11",0)="V IMMUNIZATION^9000010.11IP"
^DIC("9000010.11",0,"DD")="@"
^DIC("9000010.11",0,"GL")="^AUPNVIMM("
^DIC("9000010.11","%",0)="^1.005^^0"
^DIC("9000010.11","%D",0)="^^18^18^3160328^"
^DIC("9000010.11","%D",1,0)="In order to preserve the continuity of files shared by the Indian Health "
^DIC("9000010.11","%D",2,0)="Service and the Department of Veterans Affairs, this file includes fields "
^DIC("9000010.11","%D",3,0)="used by Indian Health Service that will not be used by the Department of "
^DIC("9000010.11","%D",4,0)="Veterans Affairs. These fields may point to other files and routines not "
^DIC("9000010.11","%D",5,0)="deployed by the Department of Veterans Affairs. Inclusion of these fields, "
^DIC("9000010.11","%D",6,0)="as designed, was approved by the DBA.  These fields were initially "
^DIC("9000010.11","%D",7,0)="introduced with the release of the VISTA IMMUNIZATION ENHANCEMENTS 1.0 "
^DIC("9000010.11","%D",8,0)="(PX*1*201) for the VistA Immunization Enhancements Project.  Additional "
^DIC("9000010.11","%D",9,0)="fields may be included in later enhancements.  "
^DIC("9000010.11","%D",10,0)="  "
^DIC("9000010.11","%D",11,0)="This file contains immunizations specific to a particular visit for a "
^DIC("9000010.11","%D",12,0)="particular patient.  This file contains one record for each immunization."
^DIC("9000010.11","%D",13,0)=" "
^DIC("9000010.11","%D",14,0)="In the VA, if an immunization is entered into PCE that has a related CPT"
^DIC("9000010.11","%D",15,0)="or ICD code, then a V CPT or V POV entry will automatically be created"
^DIC("9000010.11","%D",16,0)="with the CPT or ICD code for the immunization.  The CODING SYSTEM multiple"
^DIC("9000010.11","%D",17,0)="of the IMMUNIZATION file (#9999999.14) contains the definitions of what"
^DIC("9000010.11","%D",18,0)="immunization is related to what CPT and ICD codes."
^DIC("9000010.11","%MSC")="3161118.17223"
^DIC("9000010.12",0)="V SKIN TEST^9000010.12IP"
^DIC("9000010.12",0,"DD")="@"
^DIC("9000010.12",0,"GL")="^AUPNVSK("
^DIC("9000010.12","%",0)="^1.005^^0"
^DIC("9000010.12","%D",0)="^^14^14^3160328^"
^DIC("9000010.12","%D",1,0)="This file has been designed for joint use by the Indian Health Service and"
^DIC("9000010.12","%D",2,0)="the Department of Veteran Affairs. There will be one record for each type"
^DIC("9000010.12","%D",3,0)="of skin test given to a patient on a given visit.  Data must exist for a"
^DIC("9000010.12","%D",4,0)="patient and a visit before data can be entered here.  The record is"
^DIC("9000010.12","%D",5,0)="normally created when a skin test is given, and the results, if available,"
^DIC("9000010.12","%D",6,0)="are entered at a later date and matched to the original record.  If"
^DIC("9000010.12","%D",7,0)="results are entered and a skin test given does not exist, a new record is"
^DIC("9000010.12","%D",8,0)="created."
^DIC("9000010.12","%D",9,0)=" "
^DIC("9000010.12","%D",10,0)="In the VA, if a skin test is entered into PCE that has a related CPT or"
^DIC("9000010.12","%D",11,0)="ICD code, then a CPT or ICD entry will automatically be entered into the V"
^DIC("9000010.12","%D",12,0)="CPT or V POV file.  This supports getting workload credit from clinical"
^DIC("9000010.12","%D",13,0)="activities. The CODING SYSTEM multiple of the SKIN TEST file (#9999999.28)"
^DIC("9000010.12","%D",14,0)="defines the relationships between Skin Tests and CPT and ICD codes."
^DIC("9000010.12","%MSC")="3161118.171251"
^DIC("9000010.13",0)="V EXAM^9000010.13IP"
^DIC("9000010.13",0,"DD")="@"
^DIC("9000010.13",0,"GL")="^AUPNVXAM("
^DIC("9000010.13","%D",0)="^^3^3^2960809^^^^"
^DIC("9000010.13","%D",1,0)="This file contains exam information specific to a particular visit for a"
^DIC("9000010.13","%D",2,0)="particular patient. This file contains one record for each exam for each"
^DIC("9000010.13","%D",3,0)="visit."
^DIC("9000010.15",0)="V TREATMENT^9000010.15IP"
^DIC("9000010.15",0,"DD")="@"
^DIC("9000010.15",0,"GL")="^AUPNVTRT("
^DIC("9000010.15","%D",0)="^^8^8^2960809^^^"
^DIC("9000010.15","%D",1,0)="This file has been designed for joint use by the Indian Health Service and"
^DIC("9000010.15","%D",2,0)="the Department of Veteran Affairs."
^DIC("9000010.15","%D",3,0)=" "
^DIC("9000010.15","%D",4,0)="This file contains a record for each treatment provided to a patient "
^DIC("9000010.15","%D",5,0)="on a given patient visit.  There will be multiple treatment records"
^DIC("9000010.15","%D",6,0)="for the same treatment (.01) field based on the date on which it was "
^DIC("9000010.15","%D",7,0)="given.  Data must exist in the Patient/IHS file and visit file for the"
^DIC("9000010.15","%D",8,0)="patients' visit before data can be entered in the V Treatment file."
^DIC("9000010.16",0)="V PATIENT ED^9000010.16IP"
^DIC("9000010.16",0,"DD")="@"
^DIC("9000010.16",0,"GL")="^AUPNVPED("
^DIC("9000010.16","%D",0)="7^^7^7^2960809^^^"
^DIC("9000010.16","%D",1,0)="This file has been designed for joint use by the Indian Health Service and"
^DIC("9000010.16","%D",2,0)="the Department of Veteran Affairs."
^DIC("9000010.16","%D",3,0)=" "
^DIC("9000010.16","%D",4,0)="This is the file which stores the patient education given to a patient "
^DIC("9000010.16","%D",5,0)="or his responsible care giver. Data must exist in the Patient/IHS file and"
^DIC("9000010.16","%D",6,0)="Visit file for a patient visit before data can be entered in the V Patient"
^DIC("9000010.16","%D",7,0)="Ed File."
^DIC("9000010.18",0)="V CPT^9000010.18IP"
^DIC("9000010.18",0,"DD")="@"
^DIC("9000010.18",0,"GL")="^AUPNVCPT("
^DIC("9000010.18","%D",0)="^^18^18^2960809^^^"
^DIC("9000010.18","%D",1,0)="The V CPT file has been defined for joint use by the Indian Health Service"
^DIC("9000010.18","%D",2,0)="and the Department of Veteran Affairs. This is the file used to store CPT"
^DIC("9000010.18","%D",3,0)="related services performed at a visit.  Data must exist for a patient and"
^DIC("9000010.18","%D",4,0)="a visit before data can be entered in the V CPT file."
^DIC("9000010.18","%D",5,0)=" "
^DIC("9000010.18","%D",6,0)="This file is used in the VA to identify procedures that were done to a"
^DIC("9000010.18","%D",7,0)="patient at an encounter or occassion of service.  The procedures may"
^DIC("9000010.18","%D",8,0)="have been performed by a primary or secondary provider of patient"
^DIC("9000010.18","%D",9,0)="care.  Procedures checked off and scanned from ambulatory care encounter"
^DIC("9000010.18","%D",10,0)="forms are stored here to record that they were done.  Results of"
^DIC("9000010.18","%D",11,0)="procedures are not included."
^DIC("9000010.18","%D",12,0)=" "
^DIC("9000010.18","%D",13,0)="This file is restricted to procedures that have a CPT code.  The V"
^DIC("9000010.18","%D",14,0)="Treatment file is used to store procedures without CPT codes that do not"
^DIC("9000010.18","%D",15,0)="fit into any other V-file category."
^DIC("9000010.18","%D",16,0)=" "
^DIC("9000010.18","%D",17,0)="The Provider Narrative field represents the preferred text for this"
^DIC("9000010.18","%D",18,0)="procedure as defined by the clinician."
^DIC("9000010.23",0)="V HEALTH FACTORS^9000010.23IP"
