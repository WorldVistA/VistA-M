^DIC("58.6","%D",0)="^^11^11^3150830^"
^DIC("58.6","%D",1,0)="The PADE INBOUND TRANSACTIONS (#58.6) file contains Pharmacy Automation "
^DIC("58.6","%D",2,0)="Dispensing Equipment transactions received from PADE dispensing devices."
^DIC("58.6","%D",3,0)="The PADE HL7 transaction is first received by the VistA HL7 Messaging"
^DIC("58.6","%D",4,0)="application, where the PADE inbound filer is invoked to create an entry in"
^DIC("58.6","%D",5,0)="the PADE INBOUND TRANSACTIONS (#58.6)."
^DIC("58.6","%D",6,0)=" "
^DIC("58.6","%D",7,0)="This file creates several cross references designed to facilitate querying"
^DIC("58.6","%D",8,0)="and reporting of PADE data using various search criteria. Supported"
^DIC("58.6","%D",9,0)="transactions that are not associated with a patient include Load, Unload,"
^DIC("58.6","%D",10,0)="and Count. Supported transaction associated with a patient include"
^DIC("58.6","%D",11,0)="Dispense, Return, and Waste."
^DIC("58.6","%MSC")="3161006.130137"
^DIC("58.601",0)="PADE INVENTORY SYSTEM^58.601"
^DIC("58.601",0,"AUDIT")=""
^DIC("58.601",0,"DD")=""
^DIC("58.601",0,"DEL")=" "
