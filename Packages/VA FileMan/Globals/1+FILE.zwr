^DIC("8971.1","%D",16,0)="      midnight"
^DIC("8971.1","%D",17,0)=" WD - Workday"
^DIC("8971.1","%D",18,0)="      Monday through Friday excluding holidays as listed in the HOLIDAY "
^DIC("8971.1","%D",19,0)="      file #40.5"
^DIC("8971.1","%D",20,0)=" NW - Non-Workday"
