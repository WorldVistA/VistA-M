^DIC("394.72","%D",0)="^^25^25^2930927^"
^DIC("394.72","%D",1,0)="This file defines each encryption method currently supported by PDX."
^DIC("394.72","%D",2,0)=" "
^DIC("394.72","%D",3,0)=" "
^DIC("394.72","%D",4,0)="Methods must use the following parameters:"
^DIC("394.72","%D",5,0)="     STRING - String to encrypt/decrypt"
^DIC("394.72","%D",6,0)="     KEY1 - Name of primary encryption/decryption key"
^DIC("394.72","%D",7,0)="     KEY2 - Name of secondary encryption/decryption key (if required)"
^DIC("394.72","%D",8,0)=" "
^DIC("394.72","%D",9,0)="Methods must return the following:"
^DIC("394.72","%D",10,0)="     string - Encrypted or decrypted form of STRING"
^DIC("394.72","%D",11,0)="     NULL - Error"
^DIC("394.72","%D",12,0)=" "
^DIC("394.72","%D",13,0)="Sample methods:"
^DIC("394.72","%D",14,0)="     $$ECR^VAQZZZ(STRING,KEY1,KEY2) - Encrypts STRING using the"
^DIC("394.72","%D",15,0)="                                      encryption keys KEY1 & KEY2"
^DIC("394.72","%D",16,0)="     $$DCR^VAQZZZ(STRING,KEY1,KEY2) - Decrypts STRING using the"
^DIC("394.72","%D",17,0)="                                      decryption keys KEY1 & KEY2"
^DIC("394.72","%D",18,0)=" "
^DIC("394.72","%D",19,0)="Sample use:"
^DIC("394.72","%D",20,0)="     S IFN=internal file number for encryption method"
^DIC("394.72","%D",21,0)="     S STRING=""TEST"""
^DIC("394.72","%D",22,0)="     S KEY1=""ABCD1234"""
^DIC("394.72","%D",23,0)="     S KEY2=""ZYXW0987"""
^DIC("394.72","%D",24,0)="     X (""S X=""_^VAT(394.72,IFN,""ECR""))"
^DIC("394.72","%D",25,0)="     The variable X is now set to the encrypted format of STRING"
