^DIC("810.5","%D",1,0)="Patient lists in this file are created from the Reminder Due reporting option,"
^DIC("810.5","%D",2,0)="Reminder Patient List option or from Reminder Extract runs. Reminder patient"
^DIC("810.5","%D",3,0)="lists are retained for 5 years in this file before being purged. Individual"
^DIC("810.5","%D",4,0)="patient lists can have the automatic purge turned off."
^DIC("810.5","%D",5,0)=" "
^DIC("810.5","%D",6,0)=""
^DIC("810.5","%D",7,0)="Patient lists that are created from national extract runs are prefixed with"
^DIC("810.5","%D",8,0)="""VA-"". "
^DIC("810.5","%D",9,0)=" "
^DIC("810.5","%D",10,0)="Sites should name locally created patient lists according to their local naming "
^DIC("810.5","%D",11,0)="conventions. "
^DIC("810.5","%D",12,0)=""
^DIC("810.5","%D",13,0)=""
