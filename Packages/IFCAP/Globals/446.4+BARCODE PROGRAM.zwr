^PRCT(446.4,5,1,192,0)="P""Enter DESCRIPTION:\r\n""        ::Free text description"
^PRCT(446.4,5,1,193,0)="A                                ::40 char max because of fixed"
^PRCT(446.4,5,1,194,0)="D#0=[$0]                         ::record size"
