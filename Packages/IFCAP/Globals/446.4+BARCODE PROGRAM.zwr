^PRCT(446.4,5,1,237,0)="P$0"
^PRCT(446.4,5,1,238,0)="P""(D)elete, (Q)uit,""             ::Get instructions"
^PRCT(446.4,5,1,239,0)="P""or (C)ontinue..."""
