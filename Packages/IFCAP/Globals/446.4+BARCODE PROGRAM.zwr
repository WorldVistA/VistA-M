^PRCT(446.4,5,1,213,0)="                                 ::"
^PRCT(446.4,5,1,214,0)=".PMN                             ::     PMN"
^PRCT(446.4,5,1,215,0)="D$1=$0M1,5                       ::Is this really a PM number?"
^PRCT(446.4,5,1,216,0)="G$1=""-"".PMN1                     ::Apparently so"
