^PXRMD(811.4,64,1,5,0)=" ADMISSION DATE/TIME (FileMan format)"
^PXRMD(811.4,64,1,6,0)=" ADMISSION TYPE"
^PXRMD(811.4,64,1,7,0)=" ATTENDING PHYSICIAN"
^PXRMD(811.4,64,1,8,0)=" DATE (FileMan format)"
^PXRMD(811.4,64,1,9,0)=" PRIMARY PROVIDER"
^PXRMD(811.4,64,1,10,0)=" TREATING SPECIALTY"
^PXRMD(811.4,64,1,11,0)=" WARD LOCATION"
^PXRMD(811.4,64,1,12,0)=""
