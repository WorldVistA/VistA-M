^DGHBP(25.11,21,2,21,0)="Australia 1-800-354-965; "
^DGHBP(25.11,21,2,22,0)="Italy 1-800-782-655;"
^DGHBP(25.11,21,2,23,0)="United Kingdom (England and Scotland) 0800-032-7425; "
^DGHBP(25.11,21,2,24,0)="Mexico 001-877-345-8179; "
^DGHBP(25.11,21,2,25,0)="Japan 00531-13-0871; "
^DGHBP(25.11,21,2,26,0)="Costa Rica 0800-013-0759;"
^DGHBP(25.11,21,2,27,0)="Spain 900-981-776. "
^DGHBP(25.11,21,2,28,0)="  "
