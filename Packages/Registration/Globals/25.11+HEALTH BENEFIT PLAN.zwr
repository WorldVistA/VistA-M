^DGHBP(25.11,73,2,21,0)="copays."
^DGHBP(25.11,74,0)="EMPLOYEE ONLY^283^EM01001"
^DGHBP(25.11,74,1,0)="^^1^1^3200107"
^DGHBP(25.11,74,1,1,0)="Emp-O"
^DGHBP(25.11,74,2,0)="^^4^4^3200107"
^DGHBP(25.11,74,2,1,0)="Employees (contract, volunteer, or Full-time Equivalent) who are not "
^DGHBP(25.11,74,2,2,0)="Veterans. Records assign this carve out plan will not be share via"
^DGHBP(25.11,74,2,3,0)="Health Information Exchange. These records will also be assigned "
^DGHBP(25.11,74,2,4,0)="Humanitarian as the Core plan for any non-Employee activities."
^DGHBP(25.11,75,0)="COMPREHENSIVE EXTENDED CARE SERVICES COPAY EXEMPT^284^EC01001"
^DGHBP(25.11,75,1,0)="^^1^1^3200107"
^DGHBP(25.11,75,1,1,0)="Full-ECS-Ex"
^DGHBP(25.11,75,2,0)="^^10^10^3200214^"
^DGHBP(25.11,75,2,1,0)="Veterans enrolled in VA Health Care System and one of the following: "
^DGHBP(25.11,75,2,2,0)="  - Rated >= 70% Service Connected; or "
^DGHBP(25.11,75,2,3,0)="  - Rated 60% Service Connected and Unemployable due to Service "
^DGHBP(25.11,75,2,4,0)="    Connected Disability or"
^DGHBP(25.11,75,2,5,0)="  - Medal of Honor"
^DGHBP(25.11,75,2,6,0)=" "
^DGHBP(25.11,75,2,7,0)="  These Veterans will receive copay exempt Long Term Care within the VA "
^DGHBP(25.11,75,2,8,0)="and do not have to complete a form 10-10 EC to determine their copay "
^DGHBP(25.11,75,2,9,0)="status. Additionally, these Veterans are eligible for referral into "
^DGHBP(25.11,75,2,10,0)="community for Long Term Care Services."
^DGHBP(25.11,76,0)="VETERAN EXTENDED CARE SERVICES COPAY EXEMPT^285^EC01002"
^DGHBP(25.11,76,1,0)="^^1^1^3200107"
^DGHBP(25.11,76,1,1,0)="Ref-ECS-Ex"
^DGHBP(25.11,76,2,0)="^^14^14^3200214^"
^DGHBP(25.11,76,2,1,0)="Veterans enrolled in VA Health Care System and NOT one of the "
^DGHBP(25.11,76,2,2,0)="following: "
^DGHBP(25.11,76,2,3,0)="  - Rated >= 70% Service Connected; or "
^DGHBP(25.11,76,2,4,0)="  - Rated 60% Service Connected and Unemployable due to Service "
^DGHBP(25.11,76,2,5,0)="    Connected Disability or "
^DGHBP(25.11,76,2,6,0)="  - Medal of Honor"
^DGHBP(25.11,76,2,7,0)=" "
^DGHBP(25.11,76,2,8,0)="  But IS one of the following:"
^DGHBP(25.11,76,2,9,0)="  - Veteran with a compensable service-connected disability"
^DGHBP(25.11,76,2,10,0)="  - Veteran whose annual income is less than the Single Veteran"
^DGHBP(25.11,76,2,11,0)="    Pension Rate"
^DGHBP(25.11,76,2,12,0)=" "
^DGHBP(25.11,76,2,13,0)="  These Veterans are eligible for referral into community for Long Term"
^DGHBP(25.11,76,2,14,0)="  Care Services ONLY at the discretion of the VA Medical Center."
^DGHBP(25.11,77,0)="VETERAN EXTENDED CARE SERVICES COPAY REQ^286^EC01003"
^DGHBP(25.11,77,1,0)="^^1^1^3200107"
^DGHBP(25.11,77,1,1,0)="Ref-ECS-CoPay"
^DGHBP(25.11,77,2,0)="^^18^18^3200214^"
^DGHBP(25.11,77,2,1,0)="Veterans enrolled in VA Health Care System who are NOT: "
^DGHBP(25.11,77,2,2,0)="  - Rated >= 70% Service Connected; or "
^DGHBP(25.11,77,2,3,0)="  - Rated 60% Service Connected and Unemployable due to Service "
^DGHBP(25.11,77,2,4,0)="    Connected Disability"
^DGHBP(25.11,77,2,5,0)="  - Veteran with a compensable service-connected disability"
^DGHBP(25.11,77,2,6,0)="  - Veteran whose annual income is less than the Single Veteran"
^DGHBP(25.11,77,2,7,0)="    Pension Rate"
^DGHBP(25.11,77,2,8,0)="  - Medal of Honor"
^DGHBP(25.11,77,2,9,0)=" "
^DGHBP(25.11,77,2,10,0)="  These Veterans are eligible for Long Term Care services provided"
^DGHBP(25.11,77,2,11,0)="directly by VA but must make copayments. If a Veteran completes 10-10 "
^DGHBP(25.11,77,2,12,0)="EC process, then copay may be adjusted based on the results of the "
^DGHBP(25.11,77,2,13,0)="10-10 EC determination. Certain Vets will not have to complete the 10-10EC."
^DGHBP(25.11,77,2,14,0)=" "
^DGHBP(25.11,77,2,15,0)="  These Veterans are eligible for referral into community for Long Term  "
^DGHBP(25.11,77,2,16,0)="Care Services ONLY at the discretion of the VA Medical Center. Any care "
^DGHBP(25.11,77,2,17,0)="in the community that is authorized is subject to their copay determination"
^DGHBP(25.11,77,2,18,0)="status."
^DGHBP(25.11,78,0)="EXTENDED CARE SERVICES HUMANITARIAN^287^EC01004"
