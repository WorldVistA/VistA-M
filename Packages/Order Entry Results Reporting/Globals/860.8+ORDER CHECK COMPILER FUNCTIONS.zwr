^OCXS(860.8,24,"DESC",0)="^^20^20^3051020^"
^OCXS(860.8,24,"DESC",1,0)="The CrCl algorithm uses adjusted body weight if patient height is > 60 "
^OCXS(860.8,24,"DESC",2,0)="inches.  Approved by the CPRS Clinical Workgroup 8/11/04, it is based on a"
^OCXS(860.8,24,"DESC",3,0)="modified Cockcroft-Gault formula and was installed with patch OR*3*221."
^OCXS(860.8,24,"DESC",4,0)="For more information:"
^OCXS(860.8,24,"DESC",5,0)="   http://www.ascp.com/public/pubs/tcp/1999/jan/cockcroft.shtml"
^OCXS(860.8,24,"DESC",6,0)=" "
^OCXS(860.8,24,"DESC",7,0)="   CrCl (male) = (140 - age) x (adj body weight* in kg)       "
^OCXS(860.8,24,"DESC",8,0)="                 --------------------------------------"
^OCXS(860.8,24,"DESC",9,0)="                   (serum creatinine) x 72"
^OCXS(860.8,24,"DESC",10,0)="  * If patient height is not greater than 60 inches, actual body weight "
^OCXS(860.8,24,"DESC",11,0)="    is used."
^OCXS(860.8,24,"DESC",12,0)="  CrCl (female) = 0.85 x CrCl (male) "
^OCXS(860.8,24,"DESC",13,0)=" "
^OCXS(860.8,24,"DESC",14,0)="To calculate adjusted body weight, the following equations are used:"
^OCXS(860.8,24,"DESC",15,0)="Ideal body weight (IBW) = 50 kg x (for men) or 45 kg x (for women) + "
^OCXS(860.8,24,"DESC",16,0)="                          2.3 x (height in inches - 60) "
^OCXS(860.8,24,"DESC",17,0)="Adjusted body weight (Adj. BW) if the ratio of actual BW/IBW > 1.3 ="
^OCXS(860.8,24,"DESC",18,0)="   (0.3 x (Actual BW - IBW)) + IBW"
^OCXS(860.8,24,"DESC",19,0)="Adjusted body weight if the ratio of actual BW/IBW is not > 1.3 ="
^OCXS(860.8,24,"DESC",20,0)="   IBW or Actual BW (whichever is less)"
