^MAGD("2006.79",24,1,39,0)=" ; -- long id x-refs (set logic)"
^MAGD("2006.79",24,1,40,0)=" S VAX=X G SET1Q:X="""""
^MAGD("2006.79",24,1,41,0)=" F VAIX=0:0 S VAIX=$O(^DD(2.0361,.03,1,VAIX)) Q:'VAIX  X ^(VAIX,1) S X=VAX"
