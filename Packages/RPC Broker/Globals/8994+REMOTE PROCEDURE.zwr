^XWB(8994,2738,2,3,1,12,0)="  HOLDESC^{Seq#}^{Line of hold description}"
^XWB(8994,2738,2,3,1,13,0)="    Text for the HOLD DESCRIPTION field (25)"
^XWB(8994,2738,2,3,1,14,0)="    of the file #75.1."
^XWB(8994,2738,2,3,1,15,0)=" "
