^XWB(8994,2563,2,1,0)="DATA^1^256^1^1"
^XWB(8994,2563,2,1,1,0)="^8994.021^10^10^3090420^^^^"
^XWB(8994,2563,2,1,1,1,0)="'DATA' HAS THE FOLLOWING STRUCTURE: ""REQUEST^PARAM1|PARAM2"""
^XWB(8994,2563,2,1,1,2,0)="   'REQUEST' IS REQUIRED AND MUST BE A MEMBER OF ""CPT,FLDS,GLB""."
^XWB(8994,2563,2,1,1,3,0)="      'PARAM1' IS REQUIRED TO HOLD A CPT CODE IFF 'REQUEST'=""CPT""."
^XWB(8994,2563,2,1,1,4,0)="      'PARAM1' IS OPTIONAL TO HOLD FILEMAN FLAGS IFF 'REQUEST'=""FLDS""."
^XWB(8994,2563,2,1,1,5,0)="      'PARAM2' IS NOT USED IFF 'REQUEST'=""CPT""."
^XWB(8994,2563,2,1,1,6,0)="      'PARAM2' IS REQUIRED IF 'REQUEST'=""FLDS"" OR 'REQUEST'=""GLB""."
^XWB(8994,2563,2,1,1,7,0)="         IN EITHER CASE, 'PARAM2' MAY HOLD:"
^XWB(8994,2563,2,1,1,8,0)="           AN INTEGER REPRESENTING THE IMAGE IEN, OR"
^XWB(8994,2563,2,1,1,9,0)="           A CARET-DELIMITED 3 PIECE PARTIAL CASE/EXAM ID STRING, OR"
^XWB(8994,2563,2,1,1,10,0)="           A CARET-DELIMITED 4 PIECE COMPLETE CASE/EXAM ID STRING."
^XWB(8994,2563,2,"B","DATA",1)=""
