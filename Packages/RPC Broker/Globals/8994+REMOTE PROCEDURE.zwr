^XWB(8994,2852,3,19,0)=" RETURN(2)=123456789V123456|200M|USVHA|NI|T"
^XWB(8994,2852,3,20,0)="             ^LINK^2222222222V222222|200M|USVHA|NI|D"
^XWB(8994,2852,3,21,0)=" RETURN(3)=123456789V123456|200M|USVHA|NI|T"
^XWB(8994,2852,3,22,0)="             ^LINK^3333333333V333333|200M|USVHA|NI|D"
^XWB(8994,2852,3,23,0)=" RETURN(4)=123456789V123456|200M|USVHA|NI"
^XWB(8994,2852,3,24,0)="             ^DO NOT LINK^5555555555V555555^200M^USVHA^NI"
^XWB(8994,2852,3,25,0)=" RETURN(5)=234567890V234567|200M|USVHA|NI"
