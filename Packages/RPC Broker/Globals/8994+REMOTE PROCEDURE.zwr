^XWB(8994,2432,3,0)="^8994.03^2^2^3070320^^"
^XWB(8994,2432,3,1,0)="Global array of values in the format IEN^NAME[^FILE], where the"
^XWB(8994,2432,3,2,0)="third ""^""-piece applies only to the VARIABLE POINTER field type."
^XWB(8994,2432,3,3,0)="  List[0] := <number of entries>^code  where code equals:"
^XWB(8994,2432,3,4,0)="             S:set of codes   P:pointer   V:variable pointer"
^XWB(8994,2432,3,5,0)="  List[i] := value   for i=1,2,3,4,5,...,n+1 for n entries"
^XWB(8994,2432,3,6,0)="             value equals:"
^XWB(8994,2432,3,7,0)="    for set of codes field type, value = code:code meaning"
^XWB(8994,2432,3,8,0)="    for pointer field type, value = p1^p2"
^XWB(8994,2432,3,9,0)="    for variable pointer field type, value = p1^p2^p3"
^XWB(8994,2432,3,10,0)="        where for pointer type fields,"
^XWB(8994,2432,3,11,0)="          p1 = ien of pointed to file"
^XWB(8994,2432,3,12,0)="          p2 = external representation of .01 field from pointer to file"
^XWB(8994,2432,3,13,0)="          p3 = file number of pointed file [only returned for var ptr]"
^XWB(8994,2432,3,14,0)=" "
^XWB(8994,2432,3,15,0)="If problems or no records found, return List[0] = -1^message"
^XWB(8994,2433,0)="DSIC XPAR ADD WP^ADDWP^DSICXPR^1^A"
