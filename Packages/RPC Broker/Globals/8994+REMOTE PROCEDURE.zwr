^XWB(8994,2487,2,2,1,15,0)="   D = Deglycerolize"
^XWB(8994,2487,2,2,1,16,0)="   F = Freeze"
^XWB(8994,2487,2,2,1,17,0)="   I = Irradiate"
^XWB(8994,2487,2,2,1,18,0)="   L = Leukoreduce"
^XWB(8994,2487,2,2,1,19,0)="   P = Pool"
^XWB(8994,2487,2,2,1,20,0)="   R = Rejuvenate"
^XWB(8994,2487,2,2,1,21,0)="   S = Split/Divide"
^XWB(8994,2487,2,2,1,22,0)="   T = Thaw"
^XWB(8994,2487,2,2,1,23,0)="   U = Thaw/Pool Cryo"
^XWB(8994,2487,2,2,1,24,0)="   V = Volume Reduce"
^XWB(8994,2487,2,2,1,25,0)="   W = Wash"
^XWB(8994,2487,2,2,1,26,0)="PARAMS(""REACTION"") = Yes or No value if a reaction was indicated."
