^XWB(8994,2487,2,2,1,1,0)="PARAMS(""TRANSACTION ID"") = Unique record identifier"
^XWB(8994,2487,2,2,1,2,0)="PARAMS(""DFN"") = Patient identifier"
^XWB(8994,2487,2,2,1,3,0)="PARAMS(""ORDERING LOCATION"") = Ordering Institution Identifier"
