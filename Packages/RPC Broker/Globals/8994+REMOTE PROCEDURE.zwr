^XWB(8994,2852,3,7,0)="  requested ICN/VPID|SourceSystemID|SourceAssigningAuthority|IDType|IDState"
^XWB(8994,2852,3,8,0)="    ^LINK^linked Record|SourceSystemID|SourceAssigningAuthority|IDType|IDState"
^XWB(8994,2852,3,9,0)=" "
^XWB(8994,2852,3,10,0)="and/or"
^XWB(8994,2852,3,11,0)=" "
^XWB(8994,2852,3,12,0)="  requested ICN/VPID|SourceSystemID|SourceAssigningAuthority|IDType"
^XWB(8994,2852,3,13,0)="    ^DO NOT LINK^record ID^SourceSystemID^SourceAssigningAuthority^IDType"
^XWB(8994,2852,3,14,0)=" "
