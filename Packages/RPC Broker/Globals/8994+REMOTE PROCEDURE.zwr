^XWB(8994,2741,2,3,1,125,0)="The following optional parameters are also supported: BEDSECT, EXAMCAT, "
^XWB(8994,2741,2,3,1,126,0)="PRINCLIN, SERVICE, and WARD. If any of them are defined, their values "
^XWB(8994,2741,2,3,1,127,0)="replace the existing ones assigned by the RAMAG EXAM REGISTER."
^XWB(8994,2741,2,3,1,128,0)=" "
^XWB(8994,2741,2,3,1,129,0)="Records can be added to the list in any order. See the comments preceding"
^XWB(8994,2741,2,3,1,130,0)="the $$EXAMINED^RAMAG07 for more details."
^XWB(8994,2741,2,3,1,131,0)=" "
^XWB(8994,2741,2,3,1,132,0)="If you want to clear a multiple that already has a value, assign ""@"" or"
^XWB(8994,2741,2,3,1,133,0)="empty string to the parameter itself and do not set any subscripts. For"
^XWB(8994,2741,2,3,1,134,0)="example, the following construction will clear the CONTRAST MEDIA "
^XWB(8994,2741,2,3,1,135,0)="multiple: Mult[i] := 'CONTMEDIA^^@'."
^XWB(8994,2741,2,3,1,136,0)=" "
^XWB(8994,2741,2,3,1,137,0)="Example:"
^XWB(8994,2741,2,3,1,138,0)=" "
^XWB(8994,2741,2,3,1,139,0)="  with RPCBroker.Param[1] do"
^XWB(8994,2741,2,3,1,140,0)="    begin"
^XWB(8994,2741,2,3,1,141,0)="      PType := list;"
^XWB(8994,2741,2,3,1,142,0)="      Mult[1]  := 'CMUSED^^N';"
^XWB(8994,2741,2,3,1,143,0)="      Mult[2]  := 'FILMSIZE^1^7^2';"
^XWB(8994,2741,2,3,1,144,0)="      Mult[3]  := 'FILMSIZE^2^3^1';"
^XWB(8994,2741,2,3,1,145,0)="      Mult[4]  := 'PRIMCAM^^3';"
^XWB(8994,2741,2,3,1,146,0)="      Mult[5]  := 'TECH^1^2344';"
^XWB(8994,2741,2,3,1,147,0)="      Mult[6]  := 'FLAGS^^F';"
^XWB(8994,2741,2,3,1,148,0)=" "
^XWB(8994,2741,2,3,1,149,0)="      Mult[7]  := 'RDPHARMS^1';"
^XWB(8994,2741,2,3,1,150,0)="      Mult[8]  := 'RDPH-DRUG^^23';"
^XWB(8994,2741,2,3,1,151,0)="      Mult[9]  := 'RDPH-DOSE^^.002';"
^XWB(8994,2741,2,3,1,152,0)="      Mult[10] := 'RDPH-FORM^^A';"
^XWB(8994,2741,2,3,1,153,0)=" "
^XWB(8994,2741,2,3,1,154,0)="      Mult[11] := 'RDPHARMS^2';"
^XWB(8994,2741,2,3,1,155,0)="      Mult[12] := 'RDPH-DRUG^^23';"
^XWB(8994,2741,2,3,1,156,0)="      Mult[13] := 'RDPH-DOSE^^.002';"
^XWB(8994,2741,2,3,1,157,0)="      Mult[14] := 'RDPHARMS^2';    // Record closing tag is required"
^XWB(8994,2741,2,3,1,158,0)="                                   // if something else than another"
^XWB(8994,2741,2,3,1,159,0)="                                   // record of the same kind follows."
^XWB(8994,2741,2,3,1,160,0)="      Mult[15] := 'COMPLICAT^^13';"
^XWB(8994,2741,2,3,1,161,0)="    end;"
