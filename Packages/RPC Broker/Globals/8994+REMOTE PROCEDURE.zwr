^XWB(8994,2729,2,2,1,10,0)="DFN example:   100000511^PI^USVHA^500"
^XWB(8994,2729,2,2,1,11,0)="EDIPI example: 852043888^NI^USDOD^200DOD"
^XWB(8994,2729,2,"B","Patient Identifier",2)=""
^XWB(8994,2729,2,"PARAMSEQ",1,2)=""
^XWB(8994,2729,3,0)="^^11^11^3100107^"
^XWB(8994,2729,3,1,0)="This will return a list of treating facilities in the following format:"
^XWB(8994,2729,3,2,0)=" "
^XWB(8994,2729,3,3,0)="ID^IDTYPE^AssigningAuthority^AssigningFacility^IDStatus"
^XWB(8994,2729,3,4,0)=" "
^XWB(8994,2729,3,5,0)="Examples:"
^XWB(8994,2729,3,6,0)=" "
^XWB(8994,2729,3,7,0)="RETURN(1)=""27^PI^USVHA^500^H"""
^XWB(8994,2729,3,8,0)="RETURN(2)=""7169806^PI^USVHA^500^A"""
^XWB(8994,2729,3,9,0)="RETURN(3)=""^PI^USVHA^200PS"""
^XWB(8994,2729,3,10,0)="RETURN(4)=""1^NI^USDOD^200DOD^A"""
^XWB(8994,2729,3,11,0)="RETURN(5)=""2^NI^USDOD^200DOD^H"""
^XWB(8994,2730,0)="VAFC NEW NC TREATING FACILITY^NEWTF^VAFCTFU2^2^P^0^^^1"
^XWB(8994,2730,1,0)="^^7^7^3100414^"
^XWB(8994,2730,1,1,0)="This Remote Procedure Call will be used by the North Chicago Common "
^XWB(8994,2730,1,2,0)="Registration User Interface (UI).  Given a patient DFN and DOD EDIPI, the "
^XWB(8994,2730,1,3,0)="RPC adds an active Department of Defense correlation to the VistA "
^XWB(8994,2730,1,4,0)="TREATING FACILITY LIST (#391.91) file if it does not exist.  It returns a "
^XWB(8994,2730,1,5,0)="list of Treating Facilities, including Source Identifier, Identifier "
^XWB(8994,2730,1,6,0)="Type, Assigning Authority, Assigning Facility, Identifier Status, and an "
^XWB(8994,2730,1,7,0)="indicator if the entry was entered into File 391.91."
^XWB(8994,2730,2,0)="^8994.02A^2^2"
^XWB(8994,2730,2,1,0)="Vista Patient Identifier^1^255^1^1"
^XWB(8994,2730,2,1,1,0)="^^3^3^3100415^"
^XWB(8994,2730,2,1,1,1,0)="Vista Patient Identifier will be the PATIENT (#2) file IEN (aka DFN)."
^XWB(8994,2730,2,1,1,2,0)=" "
^XWB(8994,2730,2,1,1,3,0)="Example: DFN=""7168937"""
^XWB(8994,2730,2,2,0)="DOD Identifier^1^255^1^2"
^XWB(8994,2730,2,2,1,0)="^^5^5^3100415^"
^XWB(8994,2730,2,2,1,1,0)="The DOD Identifier will be EDIPI data with the following format:"
^XWB(8994,2730,2,2,1,2,0)=" "
^XWB(8994,2730,2,2,1,3,0)=" Id^IdType^AssigningAuthority^AssigningFacility"
^XWB(8994,2730,2,2,1,4,0)=" "
^XWB(8994,2730,2,2,1,5,0)=" Example: EDIPI=""852043888^NI^USDOD^200DOD"""
^XWB(8994,2730,2,"B","DOD Identifier",2)=""
^XWB(8994,2730,2,"B","Vista Patient Identifier",1)=""
^XWB(8994,2730,2,"PARAMSEQ",1,1)=""
^XWB(8994,2730,2,"PARAMSEQ",2,2)=""
^XWB(8994,2730,3,0)="^^12^12^3100415^"
^XWB(8994,2730,3,1,0)="This will return a list of treating facilities in the following format:"
^XWB(8994,2730,3,2,0)="  "
^XWB(8994,2730,3,3,0)=" Id^IdType^AssigningAuthority^AssigningFacility^IdStatus[^NEW]"
^XWB(8994,2730,3,4,0)="  "
^XWB(8994,2730,3,5,0)=" Examples:"
^XWB(8994,2730,3,6,0)="  "
^XWB(8994,2730,3,7,0)=" RESULT(1)=""7168937^PI^USVHA^500^A"""
