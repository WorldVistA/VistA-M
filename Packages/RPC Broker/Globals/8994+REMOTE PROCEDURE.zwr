^XWB(8994,1785,1,3,0)="provided criteria."
^XWB(8994,1785,2,0)="^8994.02A^6^6"
^XWB(8994,1785,2,1,0)="REGIEN^1^^1^1"
^XWB(8994,1785,2,1,1,0)="^8994.021^2^2^3030320^^"
