^XWB(8994,2739,3,22,0)=" "
^XWB(8994,2739,3,23,0)="Otherwise, 0 is returned in the Results[0]."
^XWB(8994,2740,0)="RAMAG EXAM COMPLETE^COMPLETE^RAMAGRP1^2^A^^^1"
^XWB(8994,2740,1,0)="^8994.01^3^3^3090306^^"
^XWB(8994,2740,1,1,0)="The RAMAG EXAM COMPLETE remote procedure completes the exam. It also sends"
^XWB(8994,2740,1,2,0)="required HL7 messages, sends changed order control ""XX"" to CPRS, but does"
^XWB(8994,2740,1,3,0)="not send VistA alerts regarding the exam status change."
^XWB(8994,2740,2,0)="^8994.02A^5^2"
^XWB(8994,2740,2,1,0)="RAEXAM^1^^1^1"
^XWB(8994,2740,2,1,1,0)="^8994.021^4^4^3080116^^^^"
^XWB(8994,2740,2,1,1,1,0)="String of exam/case identifiers separated by '^':"
^XWB(8994,2740,2,1,1,2,0)="  ^01: IEN of the patient in the RAD/NUC MED PATIENT file (#70)"
^XWB(8994,2740,2,1,1,3,0)="  ^02: IEN in the REGISTERED EXAMS multiple (sub-file #70.02)"
^XWB(8994,2740,2,1,1,4,0)="  ^03: IEN in the EXAMINATIONS multiple (sub-file #70.03)"
^XWB(8994,2740,2,5,0)="RAMSC^2^^^2"
^XWB(8994,2740,2,5,1,0)="^^91^91^3090306^"
^XWB(8994,2740,2,5,1,1,0)="Items of this list define miscellaneous/optional exam and/or report"
^XWB(8994,2740,2,5,1,2,0)="parameters. Each record has 3 or more pieces separated by '^':"
^XWB(8994,2740,2,5,1,3,0)=" "
^XWB(8994,2740,2,5,1,4,0)="  ^01: Parameter name"
^XWB(8994,2740,2,5,1,5,0)="  ^02: Index (for multiples and word-processing values)"
^XWB(8994,2740,2,5,1,6,0)="  ^03: Value1"
^XWB(8994,2740,2,5,1,7,0)="  ^04: Value2"
^XWB(8994,2740,2,5,1,8,0)="  ..."
^XWB(8994,2740,2,5,1,9,0)=" "
^XWB(8994,2740,2,5,1,10,0)="The following parameters are supported by this remote procedure:"
^XWB(8994,2740,2,5,1,11,0)=" "
^XWB(8994,2740,2,5,1,12,0)="  ACLHIST^{Seq#}^{Line of clinical history}"
^XWB(8994,2740,2,5,1,13,0)="    Text for the ADDITIONAL CLINICAL HISTORY field"
^XWB(8994,2740,2,5,1,14,0)="    (400) of the RAD/NUC MED REPORTS file (#74)."
^XWB(8994,2740,2,5,1,15,0)=" "
^XWB(8994,2740,2,5,1,16,0)="  FLAGS^^{flags}"
^XWB(8994,2740,2,5,1,17,0)="    Flags that control the execution (can be combined):"
^XWB(8994,2740,2,5,1,18,0)=" "
^XWB(8994,2740,2,5,1,19,0)="    F  Try to enforce the new status even if some "
^XWB(8994,2740,2,5,1,20,0)="       required fields are not populated."
^XWB(8994,2740,2,5,1,21,0)=" "
^XWB(8994,2740,2,5,1,22,0)="    S  Do not send HL7 message to speech recognition"
^XWB(8994,2740,2,5,1,23,0)="       (dictation) systems"
^XWB(8994,2740,2,5,1,24,0)=" "
^XWB(8994,2740,2,5,1,25,0)="  IMPRESSION^{Seq#}^{Line of impression text}"
^XWB(8994,2740,2,5,1,26,0)="    Text for the IMPRESSION TEXT field (300)"
^XWB(8994,2740,2,5,1,27,0)="    of the file #74."
^XWB(8994,2740,2,5,1,28,0)=" "
^XWB(8994,2740,2,5,1,29,0)="  PROBSTAT^^{text}"
^XWB(8994,2740,2,5,1,30,0)="    Value for the PROBLEM STATEMENT field (25)"
^XWB(8994,2740,2,5,1,31,0)="    of the file #74."
^XWB(8994,2740,2,5,1,32,0)=" "
^XWB(8994,2740,2,5,1,33,0)="  REPORT^{Seq#}^{Line of report text}"
^XWB(8994,2740,2,5,1,34,0)="    Text for the REPORT TEXT field (200)"
^XWB(8994,2740,2,5,1,35,0)="    of the file #74."
^XWB(8994,2740,2,5,1,36,0)=" "
^XWB(8994,2740,2,5,1,37,0)="  RPTDTE^^{date}"
^XWB(8994,2740,2,5,1,38,0)="    Date in HL7 format (TS) for the REPORTED DATE"
^XWB(8994,2740,2,5,1,39,0)="    field (8) of the file #74: YYYYMMDD. The date"
^XWB(8994,2740,2,5,1,40,0)="    must be exact. If a time part is provided, it"
^XWB(8994,2740,2,5,1,41,0)="    is ignored."
^XWB(8994,2740,2,5,1,42,0)=" "
^XWB(8994,2740,2,5,1,43,0)="  RPTSTATUS^^{status}"
^XWB(8994,2740,2,5,1,44,0)="    Internal value for the REPORT STATUS field (5)"
^XWB(8994,2740,2,5,1,45,0)="    of the file #74. Currently, only ""V"" (Verified)"
^XWB(8994,2740,2,5,1,46,0)="    and ""EF"" (Electronically Filed) codes are "
^XWB(8994,2740,2,5,1,47,0)="    supported."
^XWB(8994,2740,2,5,1,48,0)=" "
^XWB(8994,2740,2,5,1,49,0)="  TRANSCRST^^{IEN #200}"
^XWB(8994,2740,2,5,1,50,0)="    Internal value for the TRANSCRIPTIONIST"
^XWB(8994,2740,2,5,1,51,0)="    field (11) of the file #74: IEN in the NEW"
^XWB(8994,2740,2,5,1,52,0)="    PERSON file (#200)."
^XWB(8994,2740,2,5,1,53,0)=" "
^XWB(8994,2740,2,5,1,54,0)="  VERDTE^^{date}"
^XWB(8994,2740,2,5,1,55,0)="    Date/time in HL7 format (TS) for the"
^XWB(8994,2740,2,5,1,56,0)="    VERIFIED DATE field (7) of the file #74:"
^XWB(8994,2740,2,5,1,57,0)="    YYYYMMDD[HHMM[+/-ZZZZ]]. The date must be"
^XWB(8994,2740,2,5,1,58,0)="    exact."
^XWB(8994,2740,2,5,1,59,0)=" "
^XWB(8994,2740,2,5,1,60,0)="  VERPHYS^^{IEN #200}"
^XWB(8994,2740,2,5,1,61,0)="    Internal value for the VERIFYING PHYSICIAN"
^XWB(8994,2740,2,5,1,62,0)="    field (9) of the file #74: IEN in the NEW"
^XWB(8994,2740,2,5,1,63,0)="    PERSON file (#200)."
^XWB(8994,2740,2,5,1,64,0)=" "
^XWB(8994,2740,2,5,1,65,0)="The following optional parameters are also supported: BEDSECT, CMUSED, "
^XWB(8994,2740,2,5,1,66,0)="COMPLICAT, CONTMEDIA, CPTMODS, EXAMCAT, FILMSIZE, PRIMCAM, PRIMDXCODE,"
^XWB(8994,2740,2,5,1,67,0)="PRIMINTRES, PRIMINTSTF, PRINCLIN, RDPHARMS, RDPH-*, SERVICE, TECH,"
^XWB(8994,2740,2,5,1,68,0)="TECHCOMM, and WARD. If any of them are defined, their values replace the"
^XWB(8994,2740,2,5,1,69,0)="existing ones assigned by the RAMAG EXAM REGISTER and RAMAG EXAMINED."
^XWB(8994,2740,2,5,1,70,0)=" "
^XWB(8994,2740,2,5,1,71,0)="Records can be added to the list in any order. See the comments preceding"
^XWB(8994,2740,2,5,1,72,0)="the $$COMPLETE^RAMAG06 for more details."
^XWB(8994,2740,2,5,1,73,0)=" "
^XWB(8994,2740,2,5,1,74,0)="If you want to clear a multiple that already has a value, assign ""@"" or"
^XWB(8994,2740,2,5,1,75,0)="empty string to the parameter itself and do not set any subscripts. For"
^XWB(8994,2740,2,5,1,76,0)="example, the following construction will clear the CONTRAST MEDIA "
^XWB(8994,2740,2,5,1,77,0)="multiple: Mult[i] := 'CONTMEDIA^^@'."
^XWB(8994,2740,2,5,1,78,0)=" "
^XWB(8994,2740,2,5,1,79,0)="Example:"
^XWB(8994,2740,2,5,1,80,0)=" "
^XWB(8994,2740,2,5,1,81,0)="  with RPCBroker.Param[4] do"
^XWB(8994,2740,2,5,1,82,0)="    begin"
^XWB(8994,2740,2,5,1,83,0)="      PType := list;"
^XWB(8994,2740,2,5,1,84,0)="      Mult[1] := 'PRIMDXCODE^^1';"
^XWB(8994,2740,2,5,1,85,0)="      Mult[2] := 'CONTMEDIA^^N';"
^XWB(8994,2740,2,5,1,86,0)="      Mult[3] := 'REPORT^1^Report line #1';"
^XWB(8994,2740,2,5,1,87,0)="      Mult[4] := 'REPORT^2^Report line #2';"
^XWB(8994,2740,2,5,1,88,0)="      Mult[5] := 'IMPRESSION^1^Impression line #1';"
^XWB(8994,2740,2,5,1,89,0)="      Mult[6] := 'FLAGS^^FS';"
^XWB(8994,2740,2,5,1,90,0)="      Mult[7] := 'RPTDTE^^20071215';"
^XWB(8994,2740,2,5,1,91,0)="    end;"
